{
  "manifest_version": 3,
  "name": "NC PR nice link extension",
  "description": "NC PR nice link extension",
  "version": "1.0",
  "action": {
    "default_popup": "hello.html",
    "default_icon": "favicon-success-dark.png"
  },
  "content_scripts": [
    {
      "js": ["script.js"],
      "matches": [
        "https://github.com/**/pull/**"
      ]
    }
  ]
}
